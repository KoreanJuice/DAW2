{% extends 'base.html.twig' %}

{% block title %}Student register{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h2>Student register</h2>
    
    {{ form_start(formRegistroEst)}}
        {{ form_row(formRegistroEst.nombre) }}
        {{ form_row(formRegistroEst.apellidos) }}
        {{ form_row(formRegistroEst.calle) }}
        {{ form_row(formRegistroEst.provincia) }}
        {{ form_row(formRegistroEst.cp) }}
        {{ form_row(formRegistroEst.tlfno) }}
        {{ form_row(formRegistroEst.fNacimiento)}}
        {{ form_row(formRegistroEst.submit) }}
    <!--<button class="btn">Register</button>-->
    {{ form_end(formRegistroEst) }}

    {#{ form_start(registrationForm) }#}
        {#{ form_row(registrationForm.email) }#}
        {#{ form_row(registrationForm.plainPassword) }#}
        {#{ form_row(registrationForm.agreeTerms) }#}
    
    <!--<button class="btn">Register</button>-->
    {#{ form_end(registrationForm) }#}
</div>
{% endblock %}
